{{> menu id="dashboard"}}
<div class="border p-2 my-2">
    {{#each stations}}
        <div class="border p-2 my-2">
            <h4>{{name}}</h4>
            <h4>Latitude: {{lat}}</h4>
            <h4> Longitude: {{long}}</h4>
            <h4> Weather: {{weather}}</h4>
        </div>
    {{/each}}
</div>